<h1>Cat's info</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Age</th>
      <th scope="col">Color</th>
    </tr>
  </thead>
  <tbody>
    <tr>
     
      <td><%= @cat.name %></td>
      <td><%= @cat.age %></td>
      <td><%= @cat.color %></td>

     
    
 
    </tr>

  </tbody>
</table>
<a href="<%= cats_url %>">
<button type="button" class="btn btn-primary">Back</button>
</a>